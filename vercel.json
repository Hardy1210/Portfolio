{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, s-maxage=86400, stale-while-revalidate=59"
        },
        { "key": "CDN-Cache-Control", "value": "max-age=86400" },
        { "key": "Vercel-CDN-Cache-Control", "value": "max-age=31536000" }
      ]
    }
  ]
}
